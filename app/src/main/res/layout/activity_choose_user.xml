<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="satyam.snap.snapchatktt.chooseUserActivity">

    <ListView
            android:layout_width="411dp"
            android:layout_height="841dp"
            android:id="@+id/chooseUserListView"
            android:background="@drawable/bb"
            android:textAlignment="center"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline15" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline14" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/guideline16" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline17"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="0dp" android:id="@+id/guideline14"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="845dp"
            android:id="@+id/guideline15"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="0dp" android:id="@+id/guideline16"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintGuide_begin="413dp"
            android:id="@+id/guideline17"/>
</androidx.constraintlayout.widget.ConstraintLayout>